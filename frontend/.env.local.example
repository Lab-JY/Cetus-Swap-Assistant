# Sui Network (testnet | mainnet)
NEXT_PUBLIC_SUI_NETWORK=testnet

# Cetus Swap Contract Package ID (match your deployment)
NEXT_PUBLIC_CETUS_SWAP_PACKAGE_ID=0x622bf94c8095556221e3798242e7939c9ec6a5cdc59f90ee148dd0cc72e13480

# Google Client ID for zkLogin
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id

# App URL (for redirects)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Enable on-chain receipt objects (SwapReceipt / ZapReceipt)
NEXT_PUBLIC_ENABLE_RECEIPTS=true

# Cetus partner ID (for referral on Zap only)
NEXT_PUBLIC_CETUS_PARTNER_ID=

# Quote cache TTL (ms)
NEXT_PUBLIC_QUOTE_CACHE_TTL_MS=5000

# Optional: RPC fallback list (comma-separated)
NEXT_PUBLIC_SUI_RPC_URLS_MAINNET=
NEXT_PUBLIC_SUI_RPC_URLS_TESTNET=
